{ 
    "$schema": "http://json-schema.org/draft-04/schema#",
    "phone" : { 
     "type": "string",
     "minLength" : 2,
     "maxLength": 3 ,
     "pattern": "^(\\([0-9]{3}\\))?[0-9]{3}-[0-9]{4}$"
    },
    "fecha": {
      "type": "string",
      "format": "date-time"
    },
    "email":{
        "type": "string",
        "format": "email"
    }

}